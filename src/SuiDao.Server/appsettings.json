{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:2271;", // Http&客户端通讯端口
  "ServerSettings": {
    // 自定义域名
    // A类型解析 *.sd.test.cc 至当前服务器所在ip
    // 根据需要可增加减少子域名的级别
    "WebDomain": "sd.test.cc",

    // 可选，是否开启SSH，禁用后不处理Forward类型端口转发.默认false。
    "EnableForward": true
  }
}